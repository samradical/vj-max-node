/**
 * MaxModules
 */

.MaxModules {
	position: relative;
}

.MaxPatch {
	position: relative;
	background-color: var(--color-gold-dark);
	width: 100%;
	height: var(--maxmodule-height);
	border-bottom: 1px solid var(--color-grey-dark);
	cursor: pointer;
	&:hover{
		background-color: var( --color-grey-dark);
		color: var(--color-gold-dark);
	}
}

.MaxPatch-name{
	position: relative;
	height: 100%;
	width: 100%;
	padding: 5px;
	& span {
	}
}
